<shortcuts>
	<skipIfNotSupported/>
	<defaultCurrentUser/>
	<programGroup defaultName="TiTSCoin" location="startMenu"/>
	<shortcut
		name="TiTSCoin Server"
		target="$INSTALL_PATH\TiTSCoin.exe"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Start TiTSCoin server"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no"
		runAsAdministrator="true">
	</shortcut>	
	<shortcut
		name="TiTSCoin Wallet"
		target="http://localhost:4268/index.html"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Launch TiTSCoin wallet in browser"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no">
	</shortcut>	
</shortcuts>